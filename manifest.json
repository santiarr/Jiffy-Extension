{
    "name": "Jiffy",
    "version": "0.1",
    "description": "Browse and Save Gifs",
    "manifest_version": 2,
    "content_scripts":  [
      {
        "matches": [
          "<all_urls>"
        ],
        "js": ["content.js"]
      }
    ],
    "background": {
      "scripts": ["background.js"]
    },
    "browser_action":{
      "default_icon": "default_icon.png",
      "default_popup": "popup.html"
    }

}
